{
  "name": "pretender",
  "version": "1.5.1",
  "main": "./pretender.js",
  "license": "MIT",
  "scripts": {
    "pretest": "bower install",
    "test": "npm run lint && npm run jscs && npm run tests-only",
    "test-ci": "npm run pretest && npm run lint && npm run jscs && npm run tests-only-ci",
    "tests-only": "karma start --single-run",
    "tests-only-ci": "karma start --single-run --browsers PhantomJS",
    "lint": "jshint pretender.js test",
    "jscs": "jscs pretender.js test",
    "test:server": "karma start --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pretenderjs/pretender.git"
  },
  "devDependencies": {
    "bower": "^1.3.5",
    "coveralls": "^2.11.3",
    "es6-promise": "^4.0.5",
    "jscs": "^3.0.7",
    "jshint": "^2.8.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-qunit": "^1.2.1",
    "karma-sinon": "^1.0.5",
    "phantomjs": "^2.1.7",
    "qunitjs": "^2.4.0",
    "sinon": "^2.3.8"
  },
  "dependencies": {
    "fake-xml-http-request": "git+ssh://git@github.com/blake-education/FakeXMLHttpRequest.git#ffc2b9bb9dfacc5bd757be02bc15ba5a6637715b",
    "route-recognizer": "^0.3.3"
  },
  "jspm": {
    "shim": {
      "pretender": {
        "deps": [
          "route-recognizer",
          "fake-xml-http-request"
        ],
        "exports": "Pretender"
      }
    }
  }
}
